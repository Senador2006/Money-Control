{
  "name": "money-control",
  "version": "1.0.0",
  "description": "Sistema de controle financeiro pessoal",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test-connection": "node test-connection.js",
    "switch-atlas": "node switch-db.js atlas",
    "switch-local": "node switch-db.js local",
    "docker:build": "docker build -t money-control .",
    "docker:run": "docker run -p 3000:3000 money-control",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker-compose down -v --remove-orphans"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mongoose": "^8.18.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "finance",
    "money",
    "control",
    "budget"
  ],
  "author": "Seu Nome",
  "license": "MIT"
}
